{
    "manifest_version": 3,
    "name": "Cat weatheme",
    "description": "Base Level Extension",
    "version": "1.0",
   "action": {
      "default_popup": "hello.html",
      "default_icon": "smileys.png"
      
    },

    "options_page":"hello.html",
    "icons":{
      "48" : "smileys.png",
    "32": "smileys.png",
    "16":"smileys.png",
    "128":"smileys.png"
    },
    "permissions": ["tabs"]
  ,
  "key_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["key.js"]
    }
  ]
  }
